{
  "name": "spoonbill-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/vue": "^8.33.1",
    "axios": "^1.6.0",
    "core-js": "^3.6.5",
    "papaparse": "^5.3.0",
    "vue": "^3.0.0",
    "vue-gettext": "^2.1.12",
    "vue-i18n": "^8.24.2",
    "vue-router": "^3.2.0",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.6.10",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-router": "~5.0.4",
    "@vue/cli-plugin-unit-jest": "~5.0.4",
    "@vue/cli-plugin-vuex": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "@vue/vue2-jest": "",
    "babel-eslint": "^10.1.0",
    "easygettext": "^2.17.0",
    "eslint": "^7.5.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "jest-websocket-mock": "^2.2.0",
    "lint-staged": "^9.5.0",
    "mock-socket": "^9.0.3",
    "prettier": "^2.2.1",
    "sass": "~1.32.0",
    "sass-loader": "^11.1.1",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "warn",
      "no-debugger": "error",
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "max-len": [
        "error",
        {
          "code": 125
        }
      ],
      "keyword-spacing": [
        "error",
        {
          "after": true,
          "before": true
        }
      ],
      "semi": [
        "error",
        "always"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "comma-dangle": [
        "error",
        "only-multiline"
      ]
    },
    "overrides": [
      {
        "files": [
          "jest.setup.js",
          "**/__mocks__/*.{j,t}s?(x)",
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  }
}
