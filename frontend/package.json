{
  "dependencies": {
    "@sentry/vue": "^6.3.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "papaparse": "^5.3.0",
    "vue": "^2.6.11",
    "vue-gettext": "^2.1.12",
    "vue-i18n": "^8.24.2",
    "vue-router": "^3.2.0",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "easygettext": "^2.17.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "jest-websocket-mock": "^2.2.0",
    "lint-staged": "^9.5.0",
    "mock-socket": "^9.0.3",
    "node-sass": "^6.0.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.2.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "name": "spoonbill-frontend",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "gettext-compile": "gettext-compile --output src/translations/translations.json",
    "gettext-extract": "gettext-extract --removeHTMLWhitespaces --output web-app-ui.pot src/main.js $(find src -type f -name '*.vue')",
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve",
    "test:unit": "vue-cli-service test:unit"
  },
  "version": "0.1.0"
}
